{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::dm-gen-config/*"
        },
	{
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": 
		    "arn:aws:s3:::dm-gen-config"
	},

        {
            "Effect": "Allow",
            "Action": "kms:Decrypt",
            "Resource": "*"
        }
    ]
}
